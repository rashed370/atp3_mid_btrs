            <section id="sec-about" class="sec-about pt-5 pb-5">
              <div class="container">
                    <div class="card">
						<div class="card-header">
                            <i class="fa fa-plus-circle"></i> Add Support Ticket
						</div>
						<div class="card-body">
							<form>
                                <div class="form-group row">
                                    <label for="supportName" class="col-sm-2 col-form-label">Name</label>
                                    <div class="col-sm-10">
                                      <input type="text" class="form-control" id="supportName" placeholder="Enter Your Name">
                                    </div>
                                </div>
                                <input type="hidden">
                                <div class="form-group row">
                                  <label for="supportSubject" class="col-sm-2 col-form-label">Subject</label>
                                  <div class="col-sm-10">
                                    <input type="text" class="form-control" id="supportSubject" placeholder="Enter Subject">
                                  </div>
                                </div>
                                <div class="form-group row">
                                  <label for="supportDetails" class="col-sm-2 col-form-label">Details</label>
                                  <div class="col-sm-10">
                                    <textarea class="form-control" id="supportDetails"></textarea>
                                  </div>
                                </div>
                                <div class="form-group row">
                                  <div class="col-sm-10 offset-sm-2">
                                    <button type="submit" class="btn btn-primary">Create</button>
                                  </div>
                                </div>
                              </form>
						</div>
					</div>
              </div>
